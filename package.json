{
    "name": "flipp-ui",
    "version": "0.0.0",
    "license": "MIT",
    "scripts": {
        "postinstall": "cd mock-server && npm i",
        "ng": "ng",
        "serve": "ng serve --port 9002",
        "build": "ng build",
        "build:prod": "ng build --output-hashing none --extract-css --progress --prod --aot --env=prod --output-path ../public/ui --base-href /ui/ --deploy-url /ui/",
        "test": "ng test",
        "test:single": "ng test -sr -cc",
        "test:prod": "npm run lint && npm run test:single",
        "lint": "npm run saas-lint && ng lint",
        "mock-server": "cd mock-server && npm start",
        "jenkins": "npm run test:prod && npm run build:prod",
        "start": "concurrently \"npm run mock-server\" \"npm run serve\"",
        "saas-lint": "sass-lint -c .sass-lint.yml -v -q"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^4.3.3",
        "@angular/cdk": "^2.0.0-beta.8",
        "@angular/common": "^4.0.0",
        "@angular/compiler": "^4.0.0",
        "@angular/core": "^4.0.0",
        "@angular/forms": "^4.0.0",
        "@angular/http": "^4.0.0",
        "@angular/material": "^2.0.0-beta.8",
        "@angular/platform-browser": "^4.0.0",
        "@angular/platform-browser-dynamic": "^4.0.0",
        "@angular/router": "^4.0.0",
        "@ng-bootstrap/ng-bootstrap": "^1.0.0-beta.1",
        "@ngx-translate/core": "^7.1.0",
        "angular-tree-component": "^4.1.0",
        "angular2-jwt": "^0.2.3",
        "angular2-moment": "^1.7.0",
        "angular2-perfect-scrollbar": "^2.0.6",
        "angular2-text-mask": "^8.0.2",
        "classlist.js": "^1.1.20150312",
        "core-js": "^2.4.1",
        "intl": "^1.2.5",
        "karma-phantomjs-launcher": "^1.0.4",
        "lodash": "^4.17.4",
        "mydatepicker": "^2.0.26",
        "ng2-select": "^1.2.0",
        "ngx-bootstrap": "^1.8.0",
        "ngx-uploader": "^3.3.8",
        "phantomjs-prebuilt": "^2.1.14",
        "rxjs": "^5.1.0",
        "text-mask-addons": "^3.6.0",
        "zone.js": "^0.8.4"
    },
    "devDependencies": {
        "@angular/cli": "1.2.1",
        "@angular/compiler-cli": "^4.0.0",
        "@angular/language-service": "^4.0.0",
        "@types/jasmine": "~2.5.53",
        "@types/jasminewd2": "~2.0.2",
        "@types/node": "~6.0.60",
        "codelyzer": "~3.0.1",
        "concurrently": "^3.0.0",
        "jasmine-core": "~2.6.2",
        "jasmine-spec-reporter": "~4.1.0",
        "karma": "~1.7.0",
        "karma-chrome-launcher": "~2.1.1",
        "karma-cli": "~1.0.1",
        "karma-coverage-istanbul-reporter": "^1.2.1",
        "karma-jasmine": "~1.1.0",
        "karma-jasmine-html-reporter": "^0.2.2",
        "node-sass": "~4.5.3",
        "protractor": "~5.1.2",
        "sass-lint": "^1.10.2",
        "ts-node": "~3.0.4",
        "tslint": "~5.3.2",
        "tslint-config-airbnb": "^5.2.1",
        "typescript": "~2.4.2"
    },
    "engines": {
        "node": ">=6",
        "npm": ">=5"
    }
}
